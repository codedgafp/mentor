{{!
    @template local_entities/admin_entities

    This template renders the list of entities.

    Example context (json):
    {
        "listtypename": ["type1", "type2", "type3"]
    }
}}

<table id="entities-table">
    <thead>
    <tr>
        <th>
            {{# str }} dedicatedspace, local_entities {{/ str }}
        </th>
        {{# listtypename }}
            <th>
                {{ . }}
            </th>
        {{/ listtypename }}
    </tr>
    </thead>
</table>
